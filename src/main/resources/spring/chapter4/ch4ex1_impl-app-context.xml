<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">

        <bean id="singerOne" class="com.mygroup.springlearning.chapter5Aspects.service.SingerImpl"
              p:name="John Mayer" p:age="70"/>

        <bean id="singerTwo" class="com.mygroup.springlearning.chapter5Aspects.service.SingerImpl"
              p:age="8"/>

        <bean id="singerThree" class="com.mygroup.springlearning.chapter5Aspects.service.SingerImpl"
              p:name="John Mayer"/>
</beans>