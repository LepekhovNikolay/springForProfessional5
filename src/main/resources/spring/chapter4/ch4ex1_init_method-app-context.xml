<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">

    <bean id="singerOne" class="com.mygroup.springlearning.chapter4.example1.init_method.Singer"
          init-method="init" destroy-method="destroy"
          p:name="John Mayer" p:age="39"/>

    <bean id="singerTwo" class="com.mygroup.springlearning.chapter4.example1.init_method.Singer"
          init-method="init" destroy-method="destroy"
          p:age="39"/>

    <bean id="singerThree" class="com.mygroup.springlearning.chapter4.example1.init_method.Singer"
          init-method="init" destroy-method="destroy"
          p:name="John Mayer"/>


</beans>